{
    "name": "voice-book-server",
    "version": "0.0.1",
    "scripts": {
        "start": "node dist/server.js",
        "build": "npm run clear && tsc",
        "serve": "concurrently \"tsc --watch\" \"nodemon --delay 100ms dist/server.js\"",
        "clear": "rimraf server/dist",
        "test": "ts-mocha -p tsconfig.json src/**/*.spec.ts"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "fs-extra": "^11.1.1",
        "compression": "^1.7.4",
        "express": "^4.18.2",
        "unique-names-generator": "^4.7.1",
        "uuid": "^9.0.0",
        "@antmind/task-pool": "^0.2.1",
        "@ffmpeg-installer/ffmpeg": "^1.1.0",
        "@ffprobe-installer/ffprobe": "^2.1.1",
        "audioconcat": "^0.1.4",
        "fluent-ffmpeg": "^2.1.2"
    },
    "devDependencies": {
        "@types/compression": "^1.7.2",
        "@types/express": "^4.17.17",
        "@types/node": "^14.0.0",
        "@types/uuid": "^9.0.0",
        "@types/mocha": "^10.0.1",
        "@types/fluent-ffmpeg": "^2.1.21",
        "@types/fs-extra": "^11.0.1",
        "concurrently": "^8.0.1",
        "nodemon": "^2.0.22",
        "rimraf": "^5.0.0",
        "typescript": "^5.0.4",
        "source-map-support": "^0.5.21",
        "ts-mocha": "^10.0.0"
    }
}
